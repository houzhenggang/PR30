config samba samba
	option 'name'			'PR39'
	option 'workgroup'		'PR39'
	option 'description'		'PR39'
	option 'homes' 			'1'
	option 'enable'			'1'
		
config samba sambashare
	option 'name'			'PR_SD_Shared'
	option 'path'			'/mnt'
	option 'read_only'		'no'
	option 'guest_ok'		'yes'
	option 'create_mask'		'0777'
	option 'dir_mask'		'0777'
	option 'browseable'		'yes'
	option 'writable'		'yes'
	#option 'users'			'abc'
